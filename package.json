{
  "name": "robo-de-arbitragem2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "node scripts/safe-web-build.js",
    "build:next": "next build",
    "start": "next start -H 0.0.0.0 -p ${PORT:-10000}",
    "lint": "next lint",
    "dev:tracker": "ts-node-dev --respawn --transpile-only --project tsconfig.server.json websocket-server.ts",
    "start:tracker": "node dist/websocket-server.js",
    "build:tracker": "tsc --project tsconfig.server.json",
    "prisma:dev": "dotenv -- prisma",
    "prisma:deploy": "prisma generate && prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "postinstall": "prisma generate",
    "clean:spread": "ts-node scripts/cleanSpreadData.ts",
    "clean:db": "ts-node scripts/cleanupDatabase.ts",
    "clean:db:scheduled": "ts-node scripts/scheduleCleanup.ts",
    "fix:migrations": "node scripts/fix-production-migrations.js",
    "db:setup": "prisma generate",
    "build:cron": "tsc -p tsconfig.server.json && npm run copy-env",
    "start:cron": "node dist/scripts/server.js",
    "copy-env": "cp .env dist/.env || true",
    "build:worker": "node scripts/safe-production-build.js",
    "build:worker:safe": "prisma generate && tsc -p tsconfig.server.json",
    "start:worker": "node dist/websocket-server.js",
    "test:websocket": "ts-node scripts/test-websockets.ts",
    "monitor": "ts-node scripts/monitor-cron.ts",
    "build:monitor": "tsc -p tsconfig.scripts.json",
    "start:monitor": "node dist/scripts/monitor-cron.js"
  },
  "dependencies": {
    "@babel/core": "^7.27.4",
    "@babel/plugin-transform-runtime": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/runtime": "^7.27.6",
    "@prisma/client": "^5.22.0",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.30",
    "@types/node-cron": "^3.0.11",
    "@types/node-fetch": "^2.6.11",
    "@types/pg": "^8.15.4",
    "@types/ws": "^8.5.10",
    "autoprefixer": "^10.0.1",
    "axios": "^1.9.0",
    "ccxt": "^4.4.86",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "core-js": "^3.43.0",
    "decimal.js": "^10.5.0",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "gate-api": "^6.97.4",
    "geist": "^1.4.2",
    "lucide-react": "^0.363.0",
    "mexc-api": "^0.0.23",
    "next": "^14.2.30",
    "next-themes": "^0.4.6",
    "node-cron": "^4.1.1",
    "node-fetch": "^2.7.0",
    "pg": "^8.16.2",
    "pnpm": "^10.12.4",
    "postcss": "^8",
    "prisma": "^5.22.0",
    "react": "^18",
    "react-dom": "^18",
    "recharts": "^2.15.3",
    "sharp": "^0.33.0",
    "tailwind-merge": "^2.2.2",
    "tailwindcss": "^3.3.0",
    "tailwindcss-animate": "^1.0.7",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/axios": "^0.9.36",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^9.1.2",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8",
    "eslint-config-next": "14.1.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5"
  }
}
